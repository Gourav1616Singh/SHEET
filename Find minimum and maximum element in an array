pair<long long, long long> getMinMax(long long a[], int n) {
    
    long long max=0;
    long long min=a[0];
    for(int i=0;i<n;i++)
    {
        if(a[i]>max)
        max=a[i];
        else if(a[i]<min)
        min=a[i];
    }
    pair<long long,long long>p;
    p.first=min;
    p.second=max;
    return p;
}
